<?xml version="1.0" ?>
<project>
    <name>battery_only</name>
    <comp type="manager">
        <name>manager</name>
        <derived type="SimpleRegenManager">
            <period>1</period>
        </derived>
    </comp>
    <comp type="cti">
        <name>CTI</name>
        <derived>
            <voltage_regulator>Conv1</voltage_regulator>
            <voltage>375</voltage>
            <target_voltage>375</target_voltage>
            <capacitance>0.001</capacitance>
        </derived>
    </comp>
    <comp type="bank">
        <name>liion</name>
        <derived type="LiIonBattery">
            <capacity>3.1</capacity>
            <m_bank>18</m_bank>
            <n_bank>101</n_bank>
        </derived>
    </comp>
    <comp type="source">
        <name>regen</name>
        <derived type="ProfileVoltageSource">
            <voltage>375</voltage>
            <input_file>regen.txt</input_file>
        </derived>
    </comp>
     <comp type="load">
        <name>motor</name>
        <derived type="ProfileLoad">
            <voltage>375</voltage>
            <input_file>load.txt</input_file>
        </derived>
    </comp>
    <comp type="converter">
        <name>Conv1</name>
        <port_a>liion</port_a>
        <port_b>CTI</port_b>
        <derived type="SimpleConverter">
            <efficiency>0.95</efficiency>
        </derived>
    </comp>
    <comp type="converter">
        <name>Conv2</name>
        <port_a>regen</port_a>
        <port_b>CTI</port_b>
        <derived type="SimpleConverter">
            <efficiency>0.95</efficiency>
        </derived>
    </comp>
    <comp type="converter">
        <name>Conv3</name>
        <port_a>motor</port_a>
        <port_b>CTI</port_b>
        <derived type="SimpleConverter">
            <efficiency>0.95</efficiency>
        </derived>
    </comp>
    <sensor target="liion" property="state_of_charge"/>
    <sensor target="liion" property="consumption"/>
    <sensor target="motor" property="consumption"/>
    <sensor target="regen" property="generation"/>
    <cmd time="1800" type="finish" />
</project>
